pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
a = 0
blip = 0
rndcolor = rnd(4)
score = 0

function drop()
	spr(rndcolor, 64, blip)
	
end

function _draw()	
	cls()
	print("score")
	print(score)
	drop()
	colorbar()
end

function _update()
	if blip >= 125 then
	blip = 0
	rndcolor = rnd(4)
	else
	blip += 5
	end
	
	if btnp(1) then a += 20 end
	if btnp(0) then a -= 20 end
  resultcalc()
end

function colorbar()
	spr(5, 60, 115, 002, 006)
	spr(8, 20+a, 115, 002,006)
	spr(10,40+a, 115, 002,006)
	spr(12, 80+a, 115, 002,006)
	spr(14, 100+a, 115, 002,006)
end

function resultcalc()
		basket = pget(64, 120)
		blipcolor =	pget(64,110)
		
	if blip == 110 then
		getpoint()
	end
	
	if a >= 108  then
	a = -100
	end
	end
	
	function getpoint() 
	if blipcolor != basket then
	score -= 10
	sfx(1)
	else
	score +=10
	sfx(0)
	end
	end
